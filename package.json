{
  "name": "enjoy-events-front",
  "version": "0.0.1",
  "description": "Eventos",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:arq1nnySu/enjoy-events-front"
  },
  "authors": [],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "start": "npm run build && node server.js .",
    "build": "npm run build-js && npm run build-css",
    "watch": "npm run watch-js & npm run watch-css & serve .",
    "build-css": "rework-npm index.css | cleancss -o build/build.css",
    "build-js": "browserify --extension=.jsx --extension=.js -t babelify -t envify src/app.jsx | uglifyjs > build/build.js",
    "watch-js": "watchify --extension=.jsx --extension=.js src/app.jsx -o build/build.js --debug --verbose",
    "watch-css": "nodemon -e css --ignore build/build.css --exec 'rework-npm index.css -o build/build.css'",
    "lint-eslint": "eslint .",
    "lint-jscs": "jscs .",
    "lint": "npm run jscs"
  },
  "dependencies": {
    "babelify": "^6.1.0",
    "body-parser": "^1.4.3",
    "bootstrap": "^3.3.0",
    "browser-sync": "^2.1.6",
    "browserify": "^11.0.1",
    "clean-css": "^3.4.1",
    "envify": "^3.4.0",
    "eslint": "^0.14.1",
    "express": "^4.4.5",
    "flux": "^2.0.1",
    "jwt-decode": "^1.1.0",
    "material-ui": "^0.11.0",
    "nodemon": "^1.4.1",
    "react": "^0.13",
    "react-mfb": "^0.3.0",
    "react-mixin": "^1.1.0",
    "react-router": "^0.13.2",
    "react-tap-event-plugin": "^0.1.7",
    "reqwest": "^1.1.5",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "uglify-js": "^2.4.24",
    "watchify": "^2.1.1",
    "when": "^3.7.2"
  }
}
